      "test:watch": "jest --config jest.config.js --watch",
      "test:coverage": "jest --config jest.config.js --coverage",
      "migration:start": "yarn schema:drop && yarn schema:sync && yarn migration:run",
      "lint": "eslint 'src/**/*.{ts,tsx}' --fix"
